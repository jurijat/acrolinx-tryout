{
	"name": "acrolinx-tool",
	"private": true,
	"version": "0.0.2",
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"dev:debug": "node --inspect node_modules/vite/bin/vite.js dev",
		"build": "vite build",
		"deploy": "npm run build && npm prune --production && cf login -a https://api.cf.sap.hana.ondemand.com -o CORE_CF -s dev && cf push && npm install",
		"prepublishOnly": "npm run build",
		"preview": "vite preview",
		"prebuild": "rimraf ./dist",
		"start": "node build/index.js",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"deploy-ai-core": "tsx deploy-ai-core.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@types/inquirer": "^9.0.7",
		"@types/node": "^20.10.5",
		"tsx": "^4.7.0",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.1.8",
		"@types/crypto-js": "^4.2.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
