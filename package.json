		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-kit": "^1.2.0",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@duckdb/duckdb-wasm": "^1.29.1-dev132.0",
		"@langchain/core": "^0.3.26",
		"@sap-ai-sdk/ai-api": "^1.14.0",
		"@sap-ai-sdk/core": "^1.14.0",
		"@sap-ai-sdk/langchain": "^1.14.0",
		"@sap-ai-sdk/orchestration": "^1.14.0",
		"crypto-js": "^4.2.0",
		"dotenv": "^16.4.1",
